{
   "connection": {
        "dynamic_templates" : [
            {
                "string_fields" : {
                    "mapping" : {
                        "index" : "not_analyzed",
                        "type" : "string",
                        "doc_values": true
                    },
                    "match_mapping_type" : "string",
                    "match" : "*"
                }
            }
        ],
        "_timestamp": {
                "enabled": true,
                "path": "lastSeenTimestamp",
                "store": true
        },
        "properties": {
            "serverside": {
                "type": "boolean"
            },
            "firstSeenTimestamp": {
                "format": "date_hour_minute_second",
                "type": "date"
            },
            "lastSeenTimestamp": {
                "format": "date_hour_minute_second",
                "type": "date"
            },
            "curves_fallback": {
                "type": "boolean"
            },
            "ciphersuite": {
                "properties": {
                    "ocsp_stapling": {
                        "type": "boolean"
                    },
                    "pubkey": {
                        "type": "long"
                    }
                }
            }
        }
    }
}
