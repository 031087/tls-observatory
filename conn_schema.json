{
   "connection": {
        "_timestamp": {
                "enabled": true,
                "path": "lastSeenTimestamp",
                "store": true
        },
        "properties": {
            "serverside": {
                "type": "boolean"
            },
            "scanTarget": {
                "fields": {
                        "raw": {
                            "index": "not_analyzed",
                            "type": "string"
                        }
                },
                "type": "string"
            },
            "firstSeenTimestamp": {
                "format": "date_hour_minute_second",
                "type": "date"
            },
            "lastSeenTimestamp": {
                "format": "date_hour_minute_second",
                "type": "date"
            },
            "curves_fallback": {
                "type": "boolean"
            },
            "ciphersuite": {
                "properties": {
                    "cipher": {
                        "type": "string"
                    },
                    "ocsp_stapling": {
                        "type": "boolean"
                    },
                    "pfs": {
                        "type": "string"
                    },
                    "protocols": {
                        "type": "string"
                    },
                    "pubkey": {
                        "type": "long"
                    },
                    "sigalg": {
                        "type": "string"
                    },
                    "ticket_hint": {
                        "type": "string"
                    },
                    "curves": {
                        "type": "string"
                    }
                }
            }
        }
    }
}